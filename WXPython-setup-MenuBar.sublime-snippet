class MenuBar(wx.MenuBar):
	def __init__(self):
		wx.MenuBar.__init__(self)
		#file Menu
		file = wx.Menu()
		file.Append(-1,"Open","Open File")
		file.Append(-1,"Quit","Quit Application")
		#append file Menu to the MenuBar
		self.Append(file,'&File')
]]></content>
